<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>

<body>
    <script>// Perulangan dengan pertambahan 1
        for (let i = 1; i <= 100; i++) {
            if (i % 2 === 0) {
                console.log(`${i} adalah GENAP`);
            } else {
                console.log(`${i} adalah GANJIL`);
            }
        }

        // Perulangan dengan pertambahan 2
        for (let i = 1; i <= 100; i += 2) {
            if (i % 3 === 0) {
                console.log(`${i} adalah 3 kelipatan 3`);
            }
        }

        // Perulangan dengan pertambahan 5
        for (let i = 1; i <= 100; i += 5) {
            if (i % 6 === 0) {
                console.log(`${i} adalah 6 kelipatan 6`);
            }
        }

        // Perulangan dengan pertambahan 9
        for (let i = 1; i <= 100; i += 9) {
            if (i % 10 === 0) {
                console.log(`${i} adalah 10 kelipatan 10`);
            }
        }
    </script>
</body>

</html>